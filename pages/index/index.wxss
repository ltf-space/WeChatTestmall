view {
  color: var(--themecolor);
}
.swiper {
  /* swiper会有自己的宽高，在这里让swiper等于图片的宽高 */
  width: 750rpx;
  height: 340rpx;
  padding: 0 10rpx;
}
.swiper .swiperImge {
  width: 100%;
}
.cateImage {
  display: flex;
  padding: 20rpx 0;
}
.cateImage navigator {
  flex: 1;
}
.cateImage navigator .cate_image {
  width: 100%;
}
.floorImage .floorTitle {
  padding-top: 20rpx;
}
.floorImage .floorTitle image {
  width: 100%;
}
.floorImage .floor_list {
  overflow: hidden;
  padding: 10rpx 0;
}
.floorImage .floor_list navigator {
  float: left;
  width: 32%;
}
.floorImage .floor_list navigator:nth-last-child(-n+4) {
  height: 24.95689655vw;
  border-left: 10rpx solid #fff;
}
.floorImage .floor_list navigator:nth-child(1) {
  margin-left: 6rpx;
}
.floorImage .floor_list navigator:nth-child(2),
.floorImage .floor_list navigator:nth-child(3) {
  border-bottom: 20rpx solid #fff;
}
.floorImage .floor_list navigator image {
  width: 100%;
  height: 100%;
}
